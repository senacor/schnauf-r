{
  "name": "hello-schnauf-ms",
  "version": "0.0.1",
  "main": "./src/index.js",
  "scripts": {
    "clean": "/bin/rm -fr build/tmp/* build/*zip",
    "lint.check": "eslint src/",
    "lint": "eslint --fix src/",
    "server": "NODE_ENV=development nodemon bin/www | bunyan",
    "test": "jest --coverage",
    "test.watch": "jest --watchAll --coverage",
    "postlint": "npm run format",
    "format": "prettier --write 'src/**/*' bin/",
    "prettier": "./scripts/prettifyChangedFiles.sh"
  },
  "dependencies": {
    "assert": "~1.4.1",
    "clarify": "~2.1.0",
    "config": "~2.0.1",
    "express": "~4.16.4",
    "express-cls-hooked": "~0.3.8",
    "express-healthcheck": "~0.1.0",
    "express-prom-bundle": "^5.1.4",
    "helmet": "^3.16.0"
  },
  "devDependencies": {
    "eslint": "~5.9.0",
    "eslint-config-standard": "~12.0.0",
    "eslint-plugin-import": "~2.16.0",
    "eslint-plugin-jest": "~22.1.0",
    "eslint-plugin-node": "~8.0.1",
    "eslint-plugin-promise": "~4.0.1",
    "eslint-plugin-standard": "~4.0.0",
    "jest": "~23.6.0",
    "nodemon": "^1.18.11",
    "pre-commit": "~1.2.2",
    "prettier": "~1.15.3",
    "supertest": "~3.4.2"
  },
  "jest": {
    "resetModules": true,
    "restoreMocks": true,
    "verbose": false,
    "collectCoverage": true,
    "coverageDirectory": "build/reports/coverage/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*testdata.*"
    ],
    "roots": [
      "src"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./jest.setup.js"
  },
  "pre-commit": [
    "lint.check",
    "test",
    "prettier"
  ]
}
